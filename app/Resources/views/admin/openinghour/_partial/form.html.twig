<div class="errors">
    {{ form_errors(form) }}
</div>
<div class="row">
    <div class="col s12 m6">
        {{ form_row(form.dayOfWeek) }}
    </div>
    <div class="col s12 m6">
        {{ form_row(form.kind) }}
    </div>
</div>
<div class="row">
    <div class="col s12 m6">
        {{ form_row(form.start) }}
    </div>
    <div class="col s12 m6">
        {{ form_row(form.end) }}
    </div>
</div>
<div class="row">
    <div class="col s12">
        {{ form_errors(form.closed) }}
        <label>
            {{ form_widget(form.closed) }}
            <span>{{ form.closed.vars.label }}</span>
        </label>
    </div>
</div>
